    gutprop:partOf gutbrain:abstract_37371676 .

gutbrain:sentence_37396336_2 a gutprop:Sentence ;
    gutprop:hasSentenceText "Although the prevalence of depression and anxiety in women is about twice that of men, very few studies have examined whether physical exercise could affect mental health differently according to sex."^^xsd:string ;
